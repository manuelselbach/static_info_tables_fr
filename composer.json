{
  "name": "mselbach/static-info-tables-fr",
  "description": "French (fr) language pack for the Static Info Tables providing localized names for countries, currencies and so on.",
  "keywords": [
    "TYPO3 CMS",
    "Static Info Tables"
  ],
  "version": "6.4.0",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Manuel Selbach",
      "role": "Developer"
    }
  ],
  "replace": {
    "static_info_tables_fr": "self.version",
    "typo3-ter/static-info-tables-fr": "self.version"
  },
  "require": {
    "php": ">=5.6",
    "typo3/cms": ">=7.6,<9.0",
    "sjbr/static-info-tables": "^6.4"
  },
  "require-dev": {
    "namelesscoder/typo3-repository-client": "^1.3",
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^5.6",
    "nimut/testing-framework": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Mselbach\\StaticInfoTablesFr\\": "Classes/"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
